<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;net5.0</TargetFrameworks>
    <Version>1.1.10</Version>
    <Authors>Hayk Manucharyan</Authors>
    <Company>Hayk Manucharyan</Company>
    <Description>Minimalistic ADO.NET entity mapper.</Description>
    <Copyright>Hayk Manucharyan</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/haykmanucharyan/themapper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/haykmanucharyan/themapper</RepositoryUrl>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
